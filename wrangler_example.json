{
  "name": "eagle-reports-pdf-extractor",
  "type": "javascript",
  "workers_dev": false,
  "compatibility_date": "2025-02-28",
  "main": "worker.js",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "EAGLE_RAG_ID_STRING": "PROGRAMNAME:RAG",
    "EAGLE_RCAD_ID_STRING": "PRELIMINARYPAGEFORCREDITAUTHORIZATIONDETAILREPORT",
    "EAGLE_RCK_ID_STRING": "Doc.#Cust#JobName",
    "EAGLE_RDJ_ID_STRING": "PROGRAM-NAME:RDJ",
    "EAGLE_RDS_ID_STRING": "PRELIMINARYPAGEFORDAILYSALESTOTALSREPORT",
    "EAGLE_RTX_ID_STRING": "PROGRAM-NAME:RTX",
    "SHAREPOINT_SITE_NAME": "YOUR SHAREPOINT SITE NAME",
    "SHAREPOINT_DRIVE_NAME": "Documents",
    "SHAREPOINT_STORE_FOLDER_NAMES": "YOUR FOLDER NAME (LIKE STORE1)"
  },
  "triggers": {
    "crons": [
      "0 13 * * *"
    ] 
  }
}